<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Rental Customer Sequence -->
        <record id="sequence_rental_customer" model="ir.sequence">
            <field name="name">Rental Customer Code</field>
            <field name="code">rental.customer.code</field>
            <field name="prefix">CUST</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Rental Product Sequence -->
        <record id="sequence_rental_product" model="ir.sequence">
            <field name="name">Rental Product Code</field>
            <field name="code">rental.product.code</field>
            <field name="prefix">PROD</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Rental Order Sequence -->
        <record id="sequence_rental_order" model="ir.sequence">
            <field name="name">Rental Order</field>
            <field name="code">rental.order</field>
            <field name="prefix">RO</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

    </data>
</odoo>